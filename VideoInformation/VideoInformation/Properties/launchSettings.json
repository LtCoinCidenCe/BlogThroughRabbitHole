{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DATABASEURL": "localhost"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:8005"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DATABASEURL": "localhost"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:8985;http://localhost:8005"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080"
      },
      "publishAllPorts": true,
      "useSSL": true
    },
    "migration add": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "ef migrations add DualInput",
      "workingDirectory": "$(ProjectDir)",
      "environmentVariables": {
        "DATABASEURL": "localhost"
      }
    },
    "database update": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "ef database update",
      "workingDirectory": "$(ProjectDir)",
      "environmentVariables": {
        "DATABASEURL": "localhost"
      }
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:24787",
      "sslPort": 44331
    }
  }
}